			<tr>
				<td style="width:120px; vertical-align:middle;">::icon::</td>
				<td>
					<table border="0" cellpadding="3" cellspacing="0">
						<tr>
							<td colspan="4" style="font-weight:bold; font-size: 14px;">
								::date::
							</td>
						</tr>
						<tr>
							<td colspan="4">
